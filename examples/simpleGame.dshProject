up.alpha(1)\n}\nif currentButton <= minButton {\ncurrentButton = MaxButton\n}\nif currentButton >= MaxButton{\ncurrentButton = minButton\n}\n}\n\npressButton = (){\nif currentButton == 0{\ncurrentScene = 2\nplaytxtGroup.alpha(0)\nendtxttGroup.alpha(0)\nthis.alpha(1)\ncontrolTxtGroup.alpha(1)\n}\nif currentButton == 1{\nGame.endLevel();\n}\n}\n/*                        */\n\n/*       PLAYER          */\n\nplayerHandle = (what){\n// 1 = left\n// 2 = up\n// 3 = down\n// 4 = right\nlet speed = 10\nif what == 1{\nthis.move(speed*-1,0)\n}\nif what == 2{\nthis.move(0,speed)\n}\nif what == 3{\nthis.move(0,speed*-1)\n}\nif what == 4{\nthis.move(10,0)\n}\n}\n/*                                   */\n\n/*   Buttons    */\n//Jump p1\nGame.button_a(!{\nif currentScene == 1{\npressButton();\n}\nif currentScene == 2{\nplayerHandle(2)\n}\npressing_p1 = true\n})\nGame.button_a_end(!{\npressing_p1 = false \n})\n//Jump p2\nGame.button_b(!{\nif currentScene == 2{\nplayerHandle(3)\n}\npressing_p2 = true\n})\nGame.button_b_end(!{\npressing_p2 = false\n})\n//left p1\nGame.button_a_left(!{\nif currentScene == 1{\ncurrentButtonSum(2)\n}\nif currentScene == 2{\nplayerHandle(1)\n}\npressing_p1_left = true\n})\nGame.button_a_left_end(!{\npressing_p1_left = false\n})\n//right p1\nGame.button_a_right(!{\nif currentScene == 1{\ncurrentButtonSum(1)\n}\nif currentScene == 2{\nplayerHandle(4)\n}\npressing_p1_right = true\n})\nGame.button_a_right_end(!{\npressing_p1_right = false\n})\n//left p2\nGame.button_b_left(!{\npressing_p2_left = true\n})\nGame.button_b_left_end(!{\npressing_p2_left = false\n})\n//right p2\nGame.button_b_right(!{\npressing_p2_right = true\n})\nGame.button_b_right_end(!{\npressing_p2_right = false\n})"}}